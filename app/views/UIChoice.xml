<Alloy>
    <Window class="container">
        <Require id="backCorner" src="backCorner" type="view"/>
        <Require id="homeCorner" src="homeCorner" type="view"/>
        <Require id="configCorner" src="configCorner" type="view"/>
        <Label id="Label_Title"/>
        <Label id="Label_wizardStep"/>
        <Label id="Label_configSelect"/>
        <Label id="Label_message"/>
        <View id="View_secondaryButtons">
            <View class="btnRow" id="View_row1">
                <View class="button-wrapper largeBtn" id="View_rokUi" onClick="rokUi">
                    <ImageView class="btnMainSecondaryEndCap largeBtn fullWidth"/>
                    <Label class="btnText rokUi" id="Label_rokUi"/>
                </View>
                <View class="button-wrapper largeBtn" id="View_hybridUi" onClick="hybridUi">
                    <ImageView class="btnMainSecondary largeBtn fullWidth"/>
                    <Label class="btnText hybridUi" id="Label_hybridUi"/>
                </View>
                <View class="button-wrapper largeBtn" id="View_webUi" onClick="webUi">
                    <ImageView class="btnMainSecondary largeBtn fullWidth"/>
                    <Label class="btnText webUi" id="Label_webUi"/>
                </View>
                <View class="button-wrapper largeBtn" id="View_nativeUi" onClick="nativeUi">
                    <ImageView class="btnMainSecondaryEndCap largeBtn fullWidth mirror"/>
                    <Label class="btnText nativeUi" id="Label_nativeUi"/>
                </View>
            </View>
        </View>
        <View class="button-wrapper" id="nxtBtn" onClick="wizardContinue">
            <ImageView class="btnMainSecondary"/>
            <Label class="btnText skip"/>
        </View>
    </Window>
</Alloy>
