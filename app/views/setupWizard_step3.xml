        <Require id="homeCorner" src="homeCorner" type="view"/>
        <Require id="configCorner" src="configCorner" type="view"/>
        <Label id="Label_wizardTitle"/>
        <Label id="Label_wizardStep"/>
        <Label id="Label_configSelect"/>
        <View id="View_loginForm">
            <View class="inputLine" id="View_userLine">
                <Label id="Label_user"/>
