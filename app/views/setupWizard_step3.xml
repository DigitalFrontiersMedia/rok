            <View class="inputLine" id="View_passLine">
                <Label id="Label_pass"/>
                <TextField id="TextField_pass"/>
            </View>
            <ImageView class="imgBtn-up" id="imgBtn_login"/>
        </View>
    </Window>
</Alloy>
