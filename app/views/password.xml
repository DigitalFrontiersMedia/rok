<Alloy>
    <View class="container">
        <View class="wrapper">
            <Label id="Label_pass"/>
            <Label id="Label_pass_prompt"/>
            <View class="textfield-wrapper">
				<TextField id="TextField_pass" onReturn="connect"/>
            </View>
            <View class="inputLine" id="View_passLine">
                <ImageView class="imgBtn-up" id="imgBtn_connect" onClick="connect"/>
                <ImageView class="imgBtn-up" id="imgBtn_cancel" onClick="cancel"/>
            </View>
        </View>
    </View>
</Alloy>
